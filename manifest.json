{
  "name": "Focus Mode",
  "description": "Only Focus on my work, Block else",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ],
  "host_permissions": ["http://*/*", "https://*/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/main.png"
    }
  },
  "icons": {
    "16": "/images/main.png"
  },
  "options_page": "options.html"
}
